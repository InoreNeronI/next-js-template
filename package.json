{
  "name": "next-js-template",
  "homepage": "https://github.com/InoreNeronI/next-js-template",
  "version": "1.0.0",
  "description": "Landing page template built with Next.js & TailwindCSS ",
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "build-prod": "run-s clean format build",
    "clean": "rm -fr .next .swc out",
    "lint": "next lint",
    "format": "eslint . --fix && prettier . --write",
    "check-types": "tsc --noEmit --pretty"
  },
  "repository": {
    "type": "git",
    "url": "https://gituhb.com/InoreNeronI/next-js-template"
  },
  "keywords": [
    "nextjs",
    "tailwindcss",
    "free"
  ],
  "license": "MIT",
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@next/bundle-analyzer": "^14.0.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "next": "^14.0.3",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "styled-jsx-plugin-postcss": "^4.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
